    "\n",
    "t = [7, 10, 25, 50, 180]\n",
    "head_obs_list = [head7, head10, head25, head50, head180]\n",
    "head_obs = np.array(head_obs_list)\n",
    "#head_obs = np.array(head_obs)\n",
    "print(head_obs[1,5])\n",
    "head_obs_array = np.concatenate([head7,head10, head25, head50, head180])"
   ]
